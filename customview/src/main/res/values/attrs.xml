<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="color">
        <attr name="title_icon" format="reference"/>
        <attr name="title_icon_is_state" format="boolean"/>
        <attr name="title" format="string"/>
        <attr name="title_size" format="dimension"/>
        <attr name="icon_src" format="reference" />
        <attr name="icon_is_state" format="boolean"/>
        <attr name="description" format="string"/>
        <attr name="description_singleLine" format="boolean"/>
        <attr name="background_corner">
            <enum name="none" value="0"/>
            <enum name="up" value="1"/>
            <enum name="down" value="2"/>
            <enum name="all" value="3"/>
        </attr>
        <attr name="all_text_color" format="color"/>
        <attr name="xml_type">
            <enum name="icon_center_vertical" value="0"/>
            <enum name="icon_height_match_parent" value="1"/>
            <enum name="icon_align_title" value="2"/>
        </attr>
        <!--  tpt   -->
        <attr name="progressDrawable" format="reference" />
        <attr name="thumb" format="reference" />
        <attr name="maxValue" format="integer" />
        <attr name="minValue" format="integer" />
        <attr name="progress" format="integer" />

        <!-- navigation -->
        <attr name="last" format="string"/>
        <attr name="current" format="string"/>
    </declare-styleable>
</resources>